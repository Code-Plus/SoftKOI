<h1>Listing Reserves</h1>

<table>
  <thead>
    <tr>
      <th>Customer</th>
      <th>Product</th>
      <th>Date</th>
      <th>Start time</th>
      <th>End time</th>
      <th>State</th>
      <th>Price reserve</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @reserves.each do |reserve| %>
      <tr>
        <td><%= reserve.customer %></td>
        <td><%= reserve.product_id %></td>
        <td><%= reserve.date %></td>
        <td><%= reserve.start_time %></td>
        <td><%= reserve.end_time %></td>
        <td><%= reserve.state %></td>
        <td><%= reserve.price_reserve_id %></td>
        <td><%= link_to 'Show', reserve %></td>
        <td><%= link_to 'Edit', edit_reserve_path(reserve) %></td>
        <td><%= link_to 'Destroy', reserve, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Reserve', new_reserve_path %>

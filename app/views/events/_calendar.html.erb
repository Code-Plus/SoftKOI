<%= month_calendar events: events do |date, events| %>
  <p class="day_of_mount"><%= date.day %></p>
  <input type="text" id="tb_date_finish" value="<%= date %>" class="tb_date hidden">
  <% events.each do |event| %>
    <div class="event_description">
      <p class="events_descriptions_data"><%= event.description %></p>
      <p class="events_descriptions_data_start_time hidden"><%=event.start_time.strftime("%H:%M %p")%></p>
      <p class="events_descriptions_data_id hidden"><%= event.id %></p>

    </div>
  <% end %>
<% end %>

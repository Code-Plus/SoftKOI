<p id="notice"><%= notice %></p>

<h1>Listing Types Clothings</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @types_clothings.each do |types_clothing| %>
      <tr>
        <td><%= types_clothing.name %></td>
        <td><%= link_to 'Show', types_clothing %></td>
        <td><%= link_to 'Edit', edit_types_clothing_path(types_clothing) %></td>
        <td><%= link_to 'Destroy', types_clothing, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Types clothing', new_types_clothing_path %>

<div style="margin: 0px 60px;">

   <% content_for :title do -%>
      Precio de Reserva <i class="fa fa-caret-down"></i>
   <% end -%>

   <% content_for :menus do -%>

      <li><h5 class="drop-title"><%= link_to "Reservas", reservations_path %></h5></li>
      <li><h5 class="drop-title"><%= link_to "Consolas", consoles_path %></h5></li>
      <li><h5 class="drop-title"><%= link_to "Consolas en baja", drop_console_consoles_path %></h5></li>

   <% end %>

   <% content_for :button do -%>
      <%= link_to new_reserve_price_path, remote: true, class: 'btn btn-success bold' do %>
         <i class="fa fa-plus"></i>
         &nbsp;Nuevo Precio
      <% end %>
   <% end -%>


   <div class="panel">

      <div class="panel-body table">
         <table id="datatable" class="table table-bordered table-striped">
            <thead>
               <tr>
                  <th>Consola</th>
                  <th>Intervalo</th>
                  <th>Valor</th>
                  <th width="140">Acciones</th>
               </tr>
            </thead>

            <tbody id="reserve_prices">
               <%= render @reserve_prices %>
            </tbody>
         </table>

      </div>
   </div>

   <%= render 'dialog' %>

</div>

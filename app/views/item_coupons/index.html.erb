<p id="notice"><%= notice %></p>

<h1>Listing Item Coupons</h1>

<table>
  <thead>
    <tr>
      <th>Sale</th>
      <th>Coupon</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @item_coupons.each do |item_coupon| %>
      <tr>
        <td><%= item_coupon.sale %></td>
        <td><%= item_coupon.coupon %></td>
        <td><%= link_to 'Show', item_coupon %></td>
        <td><%= link_to 'Edit', edit_item_coupon_path(item_coupon) %></td>
        <td><%= link_to 'Destroy', item_coupon, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Item coupon', new_item_coupon_path %>
